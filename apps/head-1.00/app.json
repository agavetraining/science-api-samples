{
   "name":"head-demo",
   "parallelism":"SERIAL",
   "version":"1.00",
   "helpURI":"http://www.gnu.org/software/coreutils/manual/html_node/head-invocation.html#head-invocation",
   "label":"Head",
   "shortDescription":"Extract first lines or bytes from a file",
   "longDescription":"This is an application you can use to inspect the beginning of a file.",
   "defaultMemory": 1,
   "defaultProcessors": 1,
   "defaultQueue": "normal",
   "deploymentPath":"apps/head-1.00",
   "deploymentSystem":"demo.storage.example.com",
   "executionSystem":"demo.execute.example.com",
   "executionType":"HPC",
   "templatePath":"wrapper.template",
   "testPath":"test/test.sh",
   "checkpointable":false,
   "modules":[],
   "tags":[
      "file",
      "coreutils",
      "gnu"
   ],
   "ontology":[],
   "inputs":[
	  {
         "id":"inputFile",
         "value": {
         	"default":"apps/head-1.00/inputs/picksumipsum.txt",
            "type":"string",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Target File",
            "description":"Select an input file",
         },
         "semantics":{
           "ontology":[
               "http://sswapmeet.sswap.info/util/TextDocument"
            ],
            "minCardinality":1,
            "maxCardinality":1,
            "fileTypes":[
               "text-0"
            ]
         }
      }
   ],
   "parameters":[
     {
         "id":"numberOfLines",
         "value":{
            "default":10,
            "type":"number",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Extract the first k lines",
            "description":"Equivalent to -n command flag",
         },
         "semantics":{
            "ontology":[
               "xs:integer"
            ]
         }
      },
      {
         "id":"numberOfBytes",
         "value":{
            "default":0,
            "type":"number",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Extract the first k bytes",
            "description":"Equivalent to --countbytes command flag. Can be specified alone or in combination with number of lines. A value of 0 indicates that no byte-wise filtering is to take place.",
         },
         "semantics":{
            "ontology":[
               "xs:string"
            ]
         }
      }
   ]
}