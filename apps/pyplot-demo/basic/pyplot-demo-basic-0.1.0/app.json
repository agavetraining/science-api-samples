{
   "name":"demo-pyplot-demo-basic",
   "parallelism":"SERIAL",
   "version":"0.1.0",
   "label":"PyPlot Demo Basic",
   "shortDescription":"Simple demo app to create a graph using Python",
   "longDescription":"Simple demo app to create a graph using Python",
   "defaultMemory": 1,
   "defaultProcessors": 1,
   "defaultQueue": "normal",
   "deploymentPath":"apps/pyplot-demo-basic-0.1.0",
   "deploymentSystem":"demo.storage.example.com",
   "executionSystem":"demo.execute.example.com",
   "executionType":"HPC",
   "templatePath":"wrapper.template",
   "testPath":"test/test.sh",
   "checkpointable":false,
   "modules":[],
   "tags":[
      "demo",
      "python",
      "tutorial",
      "plotting"
   ],
   "ontology":[],
   "inputs":[
    {
         "id":"dataset",
         "value": {
           "default":"agave://demo.storage.example.com/inputs/pyplot/dataset.csv",
            "type":"string",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Dataset",
            "description":"The dataset to plot",
         },
         "semantics":{
           "ontology":[
               "http://sswapmeet.sswap.info/mime/text/Csv"
            ],
            "minCardinality":1,
            "maxCardinality":1,
            "fileTypes":[
               "text-0"
            ]
         }
      }
   ],
   "parameters":[
     {
         "id":"numberOfLines",
         "value":{
            "default":10,
            "type":"number",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Extract the first k lines",
            "description":"Equivalent to -n command flag",
         },
         "semantics":{
            "ontology":[
               "xs:integer"
            ]
         }
      },
      {
         "id":"numberOfBytes",
         "value":{
            "default":0,
            "type":"number",
            "validator":"",
            "visible":true,
            "required":true
         },
         "details":{
            "label":"Extract the first k bytes",
            "description":"Equivalent to --countbytes command flag. Can be specified alone or in combination with number of lines. A value of 0 indicates that no byte-wise filtering is to take place.",
         },
         "semantics":{
            "ontology":[
               "xs:string"
            ]
         }
      }
   ]
}
