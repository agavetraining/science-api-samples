{
  "name": "pyplot-demo test",
  "appId": "demo-pyplot-demo-advanced-0.1.0u1",
  "inputs": {
    "dataset": "agave://data.iplantcollaborative.org/dooley/inputs/pyplot/testdata.csv"
  },
  "notifications": [{
    "url": "testuser@example.com",
    "event": "FINISHED"
  }, {
    "url": "testuser@example.com",
    "event": "FAILED",
  }, {
    "url": "http://requestb.agaveapi.co/11pbi6m1?job_id=${JOB_ID}&status=${JOB_STATUS}",
    "event": "*",
    "persistent": true
  }],
  "archive": false,
  "parameters": {
    "unpackInputs": false,
    "chartType": "bar",
    "width": 1024,
    "height": 512,
    "background": "#d96727",
    "showYLabel": true,
    "ylabel": "The Y Axis Label",
    "showXLabel": true,
    "xlabel": "The X Axis Label",
    "showLegend": true,
    "separateCharts": false
  }
}
